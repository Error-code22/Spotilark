{
  "name": "spotilark",
  "version": "0.1.2",
  "private": true,
  "main": "main.js",
  "scripts": {
    "dev": "next dev --turbopack -p 9002",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "electron-dev": "cross-env NODE_ENV=development electron .",
    "electron-build": "next build && next export",
    "pack-electron": "npm run electron-build && electron-builder --dir",
    "dist-electron": "npm run electron-build && electron-builder",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build"
  },
  "build": {
    "appId": "com.spotilark.app",
    "productName": "Spotilark",
    "copyright": "Copyright ?? 2025 Spotilark",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "preload.js",
      "out/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.music",
      "target": "dmg"
    },
    "win": {
      "target": "nsis",
      "icon": "public/icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "public/icon.png",
      "category": "Audio"
    }
  },
